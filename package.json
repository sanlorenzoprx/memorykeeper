{
  "name": "memorykeeper-monorepo",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "dev": "pnpm --parallel --stream dev",
    "db:apply-schema": "wrangler d1 execute memorykeeper-db --local --file=./backend/src/db/schema.sql",
    "build": "pnpm --filter \"./**\" build",
    "lint": "pnpm --filter \"./**\" lint"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "pnpm": "^8.6.0",
    "typescript": "^5.4.5",
    "wrangler": "^4.40.2"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.896.0",
    "@aws-sdk/s3-request-presigner": "^3.896.0",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-tabs": "^1.1.13"
  }
}